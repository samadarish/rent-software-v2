{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:path:default",
    "core:path:allow-join",
    "fs:default",
    "fs:scope-download",
    "fs:scope-download-recursive",
    "fs:allow-download-write",
    "fs:allow-write-file",
    "opener:allow-open-path",
    "opener:default"
  ]
}
